#!/bin/sh

# UN-Install mythryl globally (/usr/bin &tc).
# You'll need to be root to run this, most likely.

# Created 2007-03-15 CrT under LGPL.

if [ -x /usr/bin/runtime7 ]; then
    rm  /usr/bin/runtime7

    if [ -x /usr/bin/runtime7 ]; then
	echo "Couldn't rm /usr/bin/runtime7 -- you may need to su to root to uninstall."
	exit 1
    else
	echo "/usr/bin/runtime7 successfully removed."
    fi
fi

if [ -x /usr/bin/ld7 ]; then
    rm  /usr/bin/ld7

    if [ -x /usr/bin/ld7 ]; then
	echo "Couldn't rm /usr/bin/ld7 -- you may need to su to root to uninstall."
	exit 1
    else
	echo "/usr/bin/ld7 successfully removed."
    fi
fi

if [ -x /usr/bin/mythryl ]; then
    rm  /usr/bin/mythryl

    if [ -x /usr/bin/mythryl ]; then
	echo "Couldn't rm /usr/bin/mythryl -- you may need to su to root to uninstall."
	exit 1
    else
	echo "/usr/bin/mythryl successfully removed."
    fi
fi

if [ -x /usr/bin/mythryld ]; then
    rm  /usr/bin/mythryld

    if [ -x /usr/bin/mythryld ]; then
	echo "Couldn't rm /usr/bin/mythryld -- you may need to su to root to uninstall."
	exit 1
    else
	echo "/usr/bin/mythryld successfully removed."
    fi
fi

if [ -x /usr/bin/my ]; then
    rm  /usr/bin/my

    if [ -x /usr/bin/my ]; then
	echo "Couldn't rm /usr/bin/my -- you may need to su to root to uninstall."
	exit 1
    else
	echo "/usr/bin/my successfully removed."
    fi
fi

if [ -x /usr/bin/mythryl-gtk-slave ]; then
    rm  /usr/bin/mythryl-gtk-slave

    if [ -x /usr/bin/mythryl-gtk-slave ]; then
	echo "Couldn't rm /usr/bin/mythryl-gtk-slave -- you may need to su to root to uninstall."
	exit 1
    else
	echo "/usr/bin/mythryl-gtk-slave successfully removed."
    fi
fi

if [ -x /usr/bin/build-an-executable-mythryl-heap-image ]; then
    rm  /usr/bin/build-an-executable-mythryl-heap-image

    if [ -x /usr/bin/build-an-executable-mythryl-heap-image ]; then
	echo "Couldn't rm /usr/bin/build-an-executable-mythryl-heap-image -- you may need to su to root to uninstall."
	exit 1
    else
	echo "/usr/bin/build-an-executable-mythryl-heap-image successfully removed."
    fi
fi

echo "Uninstall complete."

exit 0
