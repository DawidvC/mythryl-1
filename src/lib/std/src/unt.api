## unt.api

# Compiled by:
#     src/lib/std/src/Base-imp.make6



###                 "Words are, of course, the most
###                  powerful drug used by mankind."
###
###                             -- Rudyard Kipling



api Unt {

    eqtype Unt;

    word_size:  Int;

    to_large_unt:     Unt -> large_unt::Unt;
    to_large_unt_x:   Unt -> large_unt::Unt;
    from_large_unt:  large_unt::Unt -> Unt;

    to_large_int:     Unt -> large_int::Int;
    to_large_int_x:   Unt -> large_int::Int;
    from_large_int:  large_int::Int -> Unt;

    to_int:     Unt -> Int;
    to_int_x:   Unt -> Int;
    from_int:   Int -> Unt;

    bitwise_or:   (Unt, Unt) -> Unt;
    bitwise_xor:  (Unt, Unt) -> Unt;
    bitwise_and:  (Unt, Unt) -> Unt;
    bitwise_not:  Unt -> Unt;

    << : ((Unt, unt::Unt)) -> Unt;
    >> : ((Unt, unt::Unt)) -> Unt;
    >>> : ((Unt, unt::Unt)) -> Unt;

    + : (Unt, Unt) -> Unt;
    - : (Unt, Unt) -> Unt;
    * : (Unt, Unt) -> Unt;
    / : (Unt, Unt) -> Unt;
    % : (Unt, Unt) -> Unt;

    compare:  (Unt, Unt) -> Order;
    >  : (Unt, Unt) -> Bool;
    >= : (Unt, Unt) -> Bool;
    <  : (Unt, Unt) -> Bool;
    <= : (Unt, Unt) -> Bool;

    (-_):  Unt -> Unt;
     min:  ((Unt, Unt)) -> Unt;
     max:  ((Unt, Unt)) -> Unt;

    scan :
	 string_conversion::Radix
           -> string_conversion::Reader (Char, X)
	   -> string_conversion::Reader (Unt,  X);

    from_string:  String -> Null_Or( Unt );

    format:  string_conversion::Radix -> Unt -> String;
    to_string:    Unt -> String;

};




