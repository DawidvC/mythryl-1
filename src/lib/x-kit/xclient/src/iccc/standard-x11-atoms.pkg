	    atom_cut_buffer5		= xt::XATOM 0u14;
	    atom_cut_buffer6		= xt::XATOM 0u15;
	    atom_cut_buffer7		= xt::XATOM 0u16;
	    atom_drawable		= xt::XATOM 0u17;
