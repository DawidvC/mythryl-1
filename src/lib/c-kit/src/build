#!/bin/sh
#
# Install-time build script for c-kit-lib.make6.
#
# We will normally be run as a result of a toplevel "make rest".
#
# Our main result is the freezefile src/lib/c-kit/src/c-kit-lib.make6.a7.

bin/mythryld <<EOF
    make7::freeze "src/lib/c-kit/src/c-kit-lib.make6";
EOF

if   [ -f "src/lib/c-kit/src/c-kit-lib.make6.a7" ]
then
     echo "                 src/lib/c-kit/src/build:   Successfully created src/lib/c-kit/src/c-kit-lib.make6.a7:"
     echo "                 src/lib/c-kit/src/build:   `ls -l src/lib/c-kit/src/c-kit-lib.make6.a7`"
else
     echo "                 src/lib/c-kit/src/build:   Failed to create src/lib/c-kit/src/c-kit-lib.make6.a7"
     exit 1
fi

exit 0;
