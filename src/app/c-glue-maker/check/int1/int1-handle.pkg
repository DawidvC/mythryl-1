
package int1_handle {

    local
        lh = dynamic_linkage::open_lib {
                 name   => "./int1.so",
                 global => TRUE,
                 lazy   => TRUE
             };
    in
        fun lib_handle s
            =
            {   sh = dynamic_linkage::lib_symbol (lh, s);

                fn () =  dynamic_linkage::address sh;
            };
    end;
};
