api DOMINANCE_FRONTIERS =
api

   package dom:  Dominator_Tree

   type dominance_frontiers
       =
       rw_vector::Rw_Vector( List( graph::node_id ) )

   my DFs:  Dom::dominator_tree (N,E,G) -> dominance_frontiers

end

