## string-token.pkg
## A trivial implementation of tokens as strings w/o style information.

# Compiled by:
#     src/lib/prettyprint/devices/Prettyprint-Devices.make6

package string_token: (weak)  Prettyprint_Token	{	# Prettyprint_Token	is from   src/lib/prettyprint/src/prettyprint-token.api

    Style = Void;
    Token = String;
    fun string s = s;
    fun style _ = ();
    fun size s = string::length s;
};


