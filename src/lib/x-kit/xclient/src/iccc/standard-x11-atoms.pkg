## standard-x11-atoms.pkg
#
# Atoms are short integer representations
# of strings maintained by the X server.
#
# Here we implement the standard set of
# atoms defined by the X Inter-Client
# Communication Convention (ICCC),
# extracted from X11/Xatom.h.
#
# We use atom_primary to label standard
# atom PRIMARY etc.
#
# See also:
#
#     src/lib/x-kit/xclient/src/iccc/atom-imp.pkg
#     src/lib/x-kit/xclient/src/iccc/atom.pkg

# Compiled by:
#     src/lib/x-kit/xclient/xclient-internals.make6





stipulate

    package xt = xtypes;

herein

    package standard_x11_atoms
	=
	package {

	    atom_primary		= xt::XATOM 0u1;
	    atom_secondary		= xt::XATOM 0u2;
	    atom_arc			= xt::XATOM 0u3;
	    atom_atom			= xt::XATOM 0u4;
	    atom_bitmap			= xt::XATOM 0u5;
	    atom_cardinal		= xt::XATOM 0u6;
