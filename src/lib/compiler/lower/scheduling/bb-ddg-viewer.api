api BASIC_BLOCK_SCHEDULER_DDG_VIEWER =
api

   package ddg:  SCHEDULER_DDG
   package i:    Instruction_Set
     sharing DDG::I = I

   my to_string:  i::instruction -> String

   my view:   DDG::ddg( i::instruction, DDG::latency) -> Void

end

