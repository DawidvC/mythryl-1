#!/usr/bin/mythryl

class test_class {

    class super = object;

    field field1:     String;

    method Self(X) -> String
	get self =  "string_value";
};

class test_subclass {

    class super = test_class;

    field field2:     String;

    method Self(X) -> String
	get self =  "string_value2";
};
