## std-attributes.pkg



# Predefined attribute names.


package standard_attributes:  api

    type attribute_name = quark::quark

    my font:  attribute_name		#  "font" 
    my attribute_fontList:  attribute_name	#  "fontList" 
    my color:  attribute_name		#  "color" 
    my width:  attribute_name		#  "width" 
    my height:  attribute_name		#  "height" 
    my background:  attribute_name	#  "background" 
    my foreground:  attribute_name	#  "foreground" 
    my attribute_borderWidth:  attribute_name	#  "borderWidth" 
    my attribute_borderColor:  attribute_name	#  "borderColor" 
    my label:  attribute_name		#  "label" 
    my title:  attribute_name		#  "title" 
    my cursor:  attribute_name		#  "cursor" 

  end
{
    package q = Quark

    type attribute_name = q::quark

    font		= q::quark "font"
    attribute_fontList		= q::quark "fontList"
    color		= q::quark "color"
    width		= q::quark "width"
    height		= q::quark "height"
    background		= q::quark "background"
    foreground		= q::quark "foreground"
    attribute_borderWidth	= q::quark "borderWidth"
    attribute_borderColor	= q::quark "borderColor"
    label		= q::quark "label"
    title		= q::quark "title"
    cursor		= q::quark "cursor"

}; # package standard_attributes 



